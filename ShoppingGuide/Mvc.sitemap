        <mvcSiteMapNode title="Edit" controller="Admin" action="Edit" id="1"/>
      </mvcSiteMapNode>
      <mvcSiteMapNode title="Products" controller="ProductManage" action="Index">
        <mvcSiteMapNode title="Create" controller="ProductManage" action="Create" />
        <mvcSiteMapNode title="Delete" controller="ProductManage" action="Delete" id="1"/>
        <mvcSiteMapNode title="Details" controller="ProductManage" action="Details" id="1" />
        <mvcSiteMapNode title="Edit" controller="ProductManage" action="Edit" id="1" />
      </mvcSiteMapNode>
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Search" controller="Search" action="SearchResult" />
    <mvcSiteMapNode title="Search" controller="Search" action="SearchSort" />
