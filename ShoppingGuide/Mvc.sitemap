      <mvcSiteMapNode title="External" controller="Account" action="ExternalLoginConfirmation"/>
      <mvcSiteMapNode title="External" controller="Account" action="ExternalLoginCallback"/>
    </mvcSiteMapNode>
    <mvcSiteMapNode title="Shopping Cart" controller="ShoppingCart" action="Index" >
      <mvcSiteMapNode title="Add to Cart" controller="ShoppingCart" action="AddToCart" id="1"/>
      <mvcSiteMapNode title="Checkout" controller="Checkout" action="AddressAndPayment"/>
      <mvcSiteMapNode title="Checkout" controller="Checkout" action="Complete"/>
    </mvcSiteMapNode>
  </mvcSiteMapNode>

</mvcSiteMap>
